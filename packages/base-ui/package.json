{
  "name": "base-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/components/ui/*/index.js",
      "types": "./dist/components/ui/*/index.d.ts"
    },
    "./style": {
      "import": "./dist/assets/css/index.css",
      "require": "./dist/assets/css/index.css",
      "default": "./dist/assets/css/index.css"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*/index.js",
      "types": "./dist/hooks/*/index.d.ts"
    },
    "./lib/*": {
      "import": "./dist/lib/*/index.js",
      "types": "./dist/lib/*/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check .",
    "format": "biome format --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview-storybook": "pnpm exec http-server ./storybook-static"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/nunito-sans": "^5.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.4.1",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@phosphor-icons/react": "^2.1.10",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/addon-vitest": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "^24.10.7",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser-playwright": "4.0.17",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "playwright": "^1.57.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "storybook": "^10.2.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "4.0.17"
  },
  "peerDependencies": {
    "@phosphor-icons/react": "^2.1.10",
    "@tanstack/react-table": "^8.21.3",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^4.1.18"
  }
}
