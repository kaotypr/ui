{
  "name": "ui-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate:examples": "node scripts/generate-examples-index.mjs",
    "generate:md": "node scripts/mdx-to-md.mjs",
    "generate:llms": "node scripts/generate-llms-txt.mjs",
    "generate": "pnpm generate:examples && pnpm generate:md && pnpm generate:llms",
    "prebuild": "pnpm generate",
    "build": "next build",
    "dev": "next dev",
    "start": "serve out",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "@kaotypr/ui": "workspace:^",
    "@orama/orama": "^3.1.18",
    "@phosphor-icons/react": "^2.1.10",
    "@tanstack/react-table": "^8.21.3",
    "date-fns": "^4.1.0",
    "fumadocs-core": "16.4.9",
    "fumadocs-mdx": "14.2.6",
    "fumadocs-ui": "npm:@fumadocs/base-ui@latest",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.0.0",
    "react-dom": "^19.2.3",
    "recharts": "^2.15.0",
    "sonner": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "postcss": "^8.5.6",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
